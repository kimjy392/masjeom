action (FindMenu) {
	description (메뉴찾기)
	collect {
		input (schools) {
			type (Schools)
			min(Optional) max(One)
		}
		input (date) {
			type (Date)
			min(Optional) max(One)
		}
		input (dateTimeExpression) {
			type (time.DateTimeExpression)
			min(Optional) max(One)
			default-select {
				with-learning {
				}
			}
		}
		input (meal) {
			type (Meal)
			min(Optional) max(One)
		}
	}
	type(Search)
	output (Results) {
		on-empty {
			replace (schools) {
				intent {
					goal {Login}
				}
			}
		}
	}
}
