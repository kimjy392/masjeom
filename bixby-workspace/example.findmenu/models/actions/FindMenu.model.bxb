action (FindMenu) {
  description (메뉴찾기)
  collect {
    input (date) {
      type (Date)
      min (Required) max (One)
    }
    input (month) {
      type (Month)
      min (Optional) max (One)
    }
    input (schoolname) {
      type (SchoolName)
      min(Optional) max(One)
    }
    input (schoolkeyword) {
      type (SchoolKeyword)
      min(Optional) max(One)
    }
  }
  type(Search)
  output (Results)
}
