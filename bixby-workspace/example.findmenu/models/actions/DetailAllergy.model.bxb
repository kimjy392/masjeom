action (DetailAllergy) {
  description (__DESCRIPTION__)
  collect {
    input (date) {
      type (Date)
      min (Optional) max (One)
    }
    input (schoolname) {
      type (SchoolName)
      min (Optional) max (One)
    }
    input (menus) {
      type (Menus)
      min (Optional) max (Many)
    }
  }
  type(Search)
  output (AllergyResult) {
    evaluate {
      AllergyResult {
      date: $expr (date)
      schoolname: $expr (schoolname)
      menus: $expr (menus)
      }
    }
  }
}

