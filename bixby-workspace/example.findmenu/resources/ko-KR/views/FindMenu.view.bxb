result-view {
	match : Results(this)
	
	message {
		template ("결과")
	}
	
	render {
		if (size(this.result) > 0) {
			layout-macro (weekly) {
				param (results) {
					expression (this) // 결과 객체를 layout으로 전달합니다.
				}
			}
		}
		else {
			layout-macro (day) {
				param (results) {
					expression (this) // 결과 객체를 layout으로 전달합니다.
				}
			}
		}
	}
}

